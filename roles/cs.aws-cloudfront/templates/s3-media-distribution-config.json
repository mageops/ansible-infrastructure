        }
    },
    "Aliases": {
        {% if aws_cloudfront_domain_aliases | length > 0 %}
            "Items": ["{{ aws_cloudfront_domain_aliases | join('", "') }}"],
        {% endif %}
        "Quantity": {{ aws_cloudfront_domain_aliases | length }}
    }
}
